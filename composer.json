{
    "name": "arsentiyz/mattermost-driver",
    "description": "This Driver is a PHP implementation of the official Mattermost Web Services API.",
    "keywords": ["mattermost", "chat"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/arsentiy-byte/mattermost-driver",
    "autoload": {
        "psr-4": {
            "Arsentiyz\\MattermostDriver\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zhunussov Arsentiy",
            "email": "arsentiy.zhunussov@gmail.com"
        }
    ],
    "support": {
        "email": "arsentiyz.zhunussov@gmail.com",
        "issues": "https://github.com/arsentiy-byte/mattermost-driver/issues"
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0||^10.0||^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arsentiyz\\MattermostDriver\\MattermostServiceProvider"
            ],
            "aliases": {
                "Mattermost": "Arsentiyz\\MattermostDriver\\Facades\\Mattermost"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.63"
    }
}
